{
	"name": "pf2e-statblock-plugin",
	"version": "0.1.6",
	"description": "Allows Obsidian to render Pathfinder 2e statblocks cleanly, using only Markdown-based syntax.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Tyler Pixley",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "20.11.5",
		"@typescript-eslint/eslint-plugin": "6.19.0",
		"@typescript-eslint/parser": "6.19.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.11",
		"obsidian": "latest",
		"tslib": "2.6.2",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"@codemirror/lang-markdown": "6.2.4"
	}
}
